#!/bin/bash

# Run all tests, except ones involving blas, and matprod-norestrict.

for i in matprod matprod-off matprod-noalign \
      matprod-nosimd matprod-off-nosimd matprod-noavx \
      piped-matprod piped-matprod-noalign \
      piped-matprod-norestrict piped-matprod-nohelpers; do
  echo RUNNING TESTS FOR $i
  run-tests$1 $i >& $i.out
  tim $i
done
