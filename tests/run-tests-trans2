prog=$1-test

echo " "; echo TRANS2

echo " "; time $prog 50000$Z 2 t 522 34
echo " "; time $prog 30000$Z 2 t 1053 35
echo " "; time $prog 50000$Z 3 t 61 144
echo " "; time $prog 5000$Z 3 t 1445 62

echo " "; time $prog 100000$Z 101 t 2 100
echo " "; time $prog 500$Z 1200 t 2 1299
echo " "; time $prog 300$Z 1200 t 3 1299
echo " "; time $prog 300$Z 901 t 4 1090

echo " "; time $prog 2000000$Z 7 t 12 9
echo " "; time $prog 20000$Z 23 t 222 34
echo " "; time $prog 2000$Z 50 t 445 62
echo " "; time $prog 20000$Z 43 t 72 64
echo " "; time $prog 2000$Z 100 t 155 112

echo " "; time $prog 10$Z 430 t 520 540
echo " "; time $prog 2$Z 800 t 951 920
echo " "; time $prog 1$Z 1070 t 1140 1010
echo " "; time $prog 1$Z 1271 t 1541 1271

echo " "; time $prog 10000$Z 50 T 155 50
echo " "; time $prog 1$Z 1271 T 1541 1271
